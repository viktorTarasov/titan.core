//=========== rnd ======================
testcase tc_predef_rnd() runs on CT {
  var float vl_f := rnd();
  var float vl_seed :=3.45e+2;
  for(var integer i:=0; i<10;i:=i+1){
    vl_f := rnd();
    if( 0.0 <= vl_f and vl_f< 1.0) {
      log(vl_f)
    } else {
      setverdict(fail, vl_f);
    }
    vl_f := rnd(vl_seed);
    if( 0.0 <= vl_f and vl_f< 1.0) {
      log(vl_f)
    } else {
      setverdict(fail, vl_f);
    }
    setverdict(pass);
  }
}
